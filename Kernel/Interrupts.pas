procedure Int55Proc(var r : TRegisters);
var
EAX : word;
begin
EAX := r.EAX;

if EAX = 0045 then
  clearscreen
else if EAX = 0046 then
  PrintString('Now this is printing')

end;

procedure IntHandler(var r : TRegisters); cdecl; [public, alias: 'int_handler'];
var
s : string;
begin
if r.InterruptNumber = 55 then
  Int55Proc(r)
else
  begin
  str(r.InterruptNumber,s);
  PrintString(#10'*** Unhandled Interrupt 0x' + s);
  end;
end;

